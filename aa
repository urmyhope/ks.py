num1 = 5
num2 = 5
print("resultado:", num1 + num2)

n3 = "8"
n4 = "5"
print("resultado:", n3 + n4)
print("resultado:", int(n3) + int(n4))

n5 = "8 "
n6 = "5x "
print("resultado:", n5 + n6)

n8 = input("digite um valor: ")
print("resultado:", n8 + "5")

n9 = int(input("digite um valor: "))
print("resultado:", n9 + int("5"))
